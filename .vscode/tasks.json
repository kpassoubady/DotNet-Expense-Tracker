{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "process",
      "command": "dotnet",
      "args": ["build", "ExpenseTracker.slnx"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "run",
      "type": "process",
      "command": "dotnet",
      "args": ["run", "--project", "src/ExpenseTracker.Web/ExpenseTracker.Web.csproj"],
      "problemMatcher": "$msCompile",
      "group": "test"
    }
  ]
}